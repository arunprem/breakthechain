    function initMap() {
            var myLatLng = {lat: 10.736290, lng: 76.029040};
            var iconBase = 'https://maps.google.com/mapfiles/ms/micons/';
            var mapIcon;
            mapIcon = "red-pushpin.png";

            var map = new google.maps.Map(document.getElementById('map'), {
                zoom: 12,
                center: myLatLng
            });

            var latLng = new google.maps.LatLng(10.736290, 76.029040);
            // Creating a marker and putting it on the map
            var marker = new google.maps.Marker({
                position: latLng,
                map: map,
                title: 'CHANGARAMKULAM PS'
            });
        }

